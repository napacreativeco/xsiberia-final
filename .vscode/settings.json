// PURPLE: 48315a
// PURPLE BRIGHTER: 704591
// WHITE: eaeaea
// GREEN: 67ed9d
// PINK: d26ef1
{
  "workbench.colorCustomizations": {
      "activityBar.background": "#000000",

      // Tabs > Active
      "tab.activeForeground": "#f3f3f3",
      "tab.activeBackground": "#48315a",
      "tab.activeBorderTop": "#000000",

      // Tabs > InActive
      "tab.inactiveBackground": "#000000",
      "tab.inactiveForeground": "#727272",

      "sideBySideEditor.horizontalBorder": "#67ed9d",

      // Title Bar
      "titleBar.activeBackground": "#191919",
      "titleBar.activeForeground": "#67ed9d",

      // Editor
      "editor.background": "#181818",
      "editor.foreground": "#9c9c9c",
      "editorBracketHighlight.foreground1": "#727272",
      "editorBracketHighlight.foreground2": "#727272",
      "editorBracketHighlight.foreground3": "#727272",
      "editorBracketHighlight.foreground4": "#727272",
      "editorBracketHighlight.foreground5": "#727272",
      "editorBracketHighlight.foreground6": "#727272",
      "editorLineNumber.foreground": "#727272",
      "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000",
      "editorError.foreground": "#ff0000",
      "editor.lineHighlightBorder": "#000000",
      "editor.lineHighlightBackground": "#000000",


      // Problems
      "problemsErrorIcon.foreground": "#eaeaea",
      "problemsInfoIcon.foreground": "#eaeaea",
      "problemsWarningIcon.foreground": "#eaeaea",
      
      // Status Bar
      "statusBar.background": "#191919",
      "statusBar.foreground": "#67ed9d",

      // Explorer
      "sideBar.background": "#181818",
      "sideBarSectionHeader.background": "#000000",
      "sideBar.modifiedResourceForeground": "#eaeaea",
      "sideBar.foreground": "#4d484e",
      "sideBar.border": "#67ed9d",
      "sideBarTitle.foreground": "#67ed9d",
      "sideBarSectionHeader.foreground": "#67ed9d",
      "sideBarSectionHeader.border": "#67ed9d",
      
      // Explorer > List
      "list.errorForeground": "#7a7a7a",
      "list.warningForeground": "#7a7a7a",
      "list.activeSelectionForeground": "#67ed9d",
      "list.activeSelectionBackground": "#000000",
      
      
      // Git
      "gitDecoration.modifiedResourceForeground": "#eaeaea",
      "gitDecoration.addedResourceForeground": "#eaeaea",
      "gitDecoration.untrackedResourceForeground": "#eaeaea",
      "gitDecoration.stageModifiedResourceForeground": "#eaeaea",
      "gitDecoration.deletedResourceForeground": "#eaeaea",
      "gitDecoration.renamedResourceForeground": "#eaeaea",
      "gitDecoration.activeForeground": "#eaeaea",
      "gitDecoration.conflictingResourceForeground": "#eaeaea",
      "gitDecoration.stageDeletedResourceForeground": "#eaeaea",
      "gitDecoration.ignoredResourceForeground": "#eaeaea",
      "gitDecoration.submoduleResourceForeground": "#eaeaea",
  },
  "editor.tokenColorCustomizations": {
      "variables": "#67ed9d",
      "comments": "#d26ef1",
      "types": "#704591",
      "strings": "#67ed9d",
      "functions": "#704591",
      "keywords": "#704591",
      "numbers": "#67ed9d",
      "textMateRules": [
          {
              "scope": "variable.name",
              "settings": {
                  "foreground": "#67ed9d"
              }
          },
          {
              "scope": "entity.name.class",
              "settings": {
                  "foreground": "#67ed9d",
                  "fontStyle": "bold"
              }
          }
      ]
  }
}
